extends layout

block content

    .w3-card-4
        header.w3-container.w3-teal
            h1 #{periodo.nome}

        .w3-container
            each compositor in compositores
                if periodo.nome === compositor.periodo
                    p #{compositor.nome}
        br

        footer.w3-container.w3-blue
            h5
                span Generated by EngWeb2024 in #{date} - [
                    a(href='/') Return
                span ]
